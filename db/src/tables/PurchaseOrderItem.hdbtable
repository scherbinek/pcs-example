column table "pcs-example.db.tables::PurchaseOrderItem"(
  "PURCHASEORDERID" nvarchar(10),
  "PURCHASEORDERITEM" nvarchar(10),
  "PRODUCTID" nvarchar(10),
  "NOTEID" nvarchar(10) DEFAULT null,
  "CURRENCY" nvarchar(5),
  "GROSSAMOUNT" double,
  "NETAMOUNT" double,
  "TAXAMOUNT" double,
  "QUANTITY" double,
  "QUANTITYUNIT" nvarchar(3),
  "DELIVERYDATE" date,
  PRIMARY KEY ("PURCHASEORDERID", "PURCHASEORDERITEM")
)